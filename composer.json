{
  "name": "mishterk/wp-tailwindcss-theme-boilerplate",
  "type": "project",
  "license": "MIT",
  "description": "WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
  "homepage": "https://philkurth.com.au/articles/wordpress-theme-using-tailwind-css/",
  "authors": [
    {
      "name": "Phil Kurth",
      "homepage": "https://philkurth.com.au"
    }
  ],
  "keywords": [
    "laravelmix", "wordpress", "wordpress-theme", "wordpress-development", "wordpress-starter-theme", "wordpress-boilerplate", "tailwindcss", "tailwind", "tailwind-css"
  ],
  "support": {
    "issues": "https://github.com/mishterk/wp-tailwindcss-theme-boilerplate/issues"
  },
  "require": {
    "php": ">=5.3.2"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "friendsofphp/php-cs-fixer": "^2.16"
  }
}
