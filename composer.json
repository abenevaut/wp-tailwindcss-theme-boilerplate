{
  "name": "abenevaut/wptheme-tailwindcss",
  "type": "project",
  "license": "GPL-3.0-only",
  "description": "WordPress theme boilerplate with tailwindcss",
  "homepage": "https://github.com/abenevaut/wptheme-tailwindcss",
  "authors": [
    {
      "name": "Phil Kurth",
      "homepage": "https://philkurth.com.au"
    },
    {
      "name": "Antoine Benevaut",
      "email": "antoine@benevaut.fr"
    }
  ],
  "keywords": [
    "wordpress", "wordpress-theme", "tailwindcss", "laravelmix"
  ],
  "support": {
    "issues": "https://github.com/abenevaut/wptheme-tailwindcss/issues"
  },
  "require": {
    "php": "^7.3"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
